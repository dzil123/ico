script/source = "extends ShapeConfig


func init():
	# https://en.wikipedia.org/wiki/Regular_icosahedron#Dimensions
	RADIUS_VERTEX = sqrt(10.0 + 2.0 * sqrt(5.0)) / 4.0
	RADIUS_FACE = (sqrt(3.0) * (3.0 + sqrt(5.0))) / 12.0
	RADIUS_EDGE = (1.0 + sqrt(5.0)) / 4.0
	ANGLE = PI - acos(sqrt(5.0) / -3.0)
	START_ROTATION = Basis().rotated(Vector3(0, 1, 0), PI / 6.0)
"

